<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.0//EN"
        "http://google-web-toolkit.googlecode.com/svn/releases/2.0/distro-source/core/src/gwt-module.dtd">
<module rename-to="app">

    <!-- Inherit the core Web Toolkit stuff.                  -->
    <inherits name='com.google.gwt.user.User'/>

    <!-- Specify the app entry point class.                   -->
    <!-- Точка входа в мое приложение с точки зрения GWT класс который должен эти компоненты создать и вставить в id -->
    <!-- занимается подготовкой инициализации функционала-->
    <entry-point class='ru.kpfu.itis.Gilmanova.gwt.client.EntryPointApp'/>

    <!-- Specify the app servlets.                   -->
    <servlet path='/QuestionService' class='ru.kpfu.itis.Gilmanova.gwt.server.QuestionServiceImpl'/>

</module>